#description="Manjaro livecd mhwd-live script"

depend() {
    need livecd
    before xdm
}

start() {
	ebegin "Running mhwd-live script"
	# run the mhwd script
	/usr/bin/mhwd-live >> /var/log/rc.log
	eend $?
}
